{
    "extends": "airbnb-base",
    "rules": {
        "indent": [2, 4],
        "max-len": [1, 120, 2, {"ignoreComments": true}],
        "linebreak-style": ["error", "windows"],
        "class-methods-use-this": 0,
        "no-underscore-dangle": "off"
    },
    "env": {
        "browser": true
    } 
}